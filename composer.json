{
    "version": "0.0.1",
    "name": "gesprender/framework",
    "description": "GesPrender Framework",
    "type": "project",
    "license": "proprietary",
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Core\\": "src/",
            "Config\\": "config/",
            "Project\\": "Project/backend",
            "Backoffice\\": "Backoffice/src"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4.0|^8.1",
        "vlucas/phpdotenv": "^5.5",
        "endroid/qr-code": "^3.9",
        "phpoffice/phpspreadsheet": "^1.19",
        "firebase/php-jwt": "^5.5.1",
        "gesprender/cli": "master",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "2.0.x-dev",
        "phpunit/phpunit": "^11.0@dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gesprender/Coreshell-CLI.git"
        }
    ]
}
